{
  "name": "cypress",
  "version": "1.0.0",
  "description": "1) install nodeJS\r 2) create folder\r 3) open this folder in vs code\r 4) check if the node js is installed with command \"node -v\"\r 5) npm init\r 6) npm install cypress --save -dev - installing\r 7) npx cypress open",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "testus": "npx cypress open",
    "pretestscript": "npx cypress run --spec cypress/e2e/iFrames.cy.js,cypress/e2e/parametrization.cy.js --browser chrome",
    "testscript": "npx cypress run --spec cypress/e2e/iFrames.cy.js,cypress/e2e/parametrization.cy.js --browser firefox",
    "posttestscript": "npx cypress run --spec cypress/e2e/iFrames.cy.js,cypress/e2e/parametrization.cy.js --browser edge",
    "start": "npx cypress open --e2e --browser chrome",
    "pom_tests:run": "npx cypress run --spec 'cypress/e2e/*.js' "
  },
  "author": "Alexey B.",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "cypress": "^13.3.1",
    "cypress-plugin-api": "^2.11.1",
    "installing": "^1.0.0",
    "playwright-webkit": "1.34"
  },
  "devDependencies": {
    "@cypress/grep": "^4.0.1",
    "cypress-mochawesome-reporter": "^3.7.0",
    "dayjs": "^1.11.10"
  }
}
